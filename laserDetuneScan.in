Dline:                         D2   # chose D1 or D2 line to simulate 
excited_hpf_state:             F1   # In case its D1 line, specify the excited hpf F
polarization1:            sigmaPlus   # polarization for transition from ground F = 1
polarization2:            sigmaPlus   # polarization for transition from ground F = 2  
I1:                            2    # [mW/cm^2]  laser intensity corresponding to polarization1
I2:                            0   # [mW/cm^2]  laser intensity corresponding to polarization1
detune1:                        0   # [Hz]  laser detune corresponding to polarization1
detune2:                        0   # [Hz]  laser detune corresponding to polarization1
scanLaserId:			I1  # 1 or 2 chose laser to scan, 1, 2 corresponding to I1, I2
startDetune:		 	-20e6   # [Hz] starting scan laser detune
endDetune:			 20e6   # [Hz] ending scan laser detune
dDetune:			 5e6   # [Hz] laser detune scan step size
dt:                          20e-9    # [s]  simulation time step size 
maxSimulationTime:           1000e-6    # [s]  max simulation time, auto stop if rearch steady state